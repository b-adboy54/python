
#swapping using a temporary variable
x = 5
y = 10
temp = x
x = y
y = temp
print('The value of x after swapping: {}'.format(x)) 
print('The value of y after swapping: {}'.format(y))



#swapping without using a temporary variable 
x = 5
y = 10
x, y = y, x 
print('The value of x after swapping: {}'.format(x)) 
print('The value of y after swapping: {}'.format(y))
